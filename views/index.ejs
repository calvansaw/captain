<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head.ejs')%>
</head>
<body>
    <div>
        <h1>All Logs - Index page</h1>
        <ul>

            <%allLogs.forEach(log => {%>
                <li><a href="/logs/<%=log._id%>"><%=log.title + ' ID:' + log._id %></a></li>

                <nav><a href="/logs/<%=log._id%>/edit">EDIT</a></nav>

                <form action="/logs/<%=log.id%>?_method=DELETE" method="POST">
                    <input type="submit" value="DELETE">
                </form>
            <%});%>

        </ul>
    </div>
    <nav><a href="/logs/new">Create New Logs</a></nav>
</body>
</html>